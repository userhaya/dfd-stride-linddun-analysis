              "threats": [
                {
                  "id": "35f01eb6-3bd6-4150-abcd-8c1a336341c4",
                  "title": "Detectable document delivery to POS user",
                  "status": "Open",
                  "severity": "Medium",
                  "type": "Detectability",
                  "description": "When a document is delivered from the PDS process to the user, the delivery event may be observable through network traffic, access logs, or monitoring systems. This allows an observer to detect that a specific document delivery or retrieval event has occurred.\n",
                  "mitigation": "Use encrypted communication (HTTPS/TLS).\n\n Avoid detailed access logging where not required.\n\n Use anonymized or pseudonymous delivery identifiers.\n\n Limit visibility of delivery events to authorized components.",
                  "modelType": "LINDDUN",
                  "new": false,
                  "number": 13,
                  "score": "4"
                }
              ]
            },
